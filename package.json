{
  "name": "Qodestek Assignment",
  "packageManager": "yarn@3.3.1",
  "version": "1.0.0",
  "private": true,
  "npmClient": "yarn",
  "useWorkspaces": true,
  "scripts": {
    "server:dev": "yarn workspace server dev",
